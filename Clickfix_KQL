let clickfix=externaldata(Filename: string, SHA256:string)[h"https://raw.githubusercontent.com/saidineshkintali/Threat-Hunting-MDE/refs/heads/main/clickfix.csv"]
clickfix
| invoke FileProfile("SHA256", 1000)
| project Filename, SHA256, GlobalPrevalence, GlobalFirstSeen, GlobalLastSeen
| Sort by GlobalPrevalence desc

